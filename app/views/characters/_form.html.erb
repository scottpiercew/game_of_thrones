<%= form_for [@character] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.label :house_id %>
  <%= f.text_field :house_id %>

  <%= f.submit "Submit" %>

<% end %>

<% @houses.each do |house| %>
  <p><%= house.name %> select </P>
    <h3><%= house.id %></h3>

<% end %>
